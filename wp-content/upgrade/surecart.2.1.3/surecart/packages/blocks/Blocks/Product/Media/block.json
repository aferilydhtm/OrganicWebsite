{
	"apiVersion": 2,
	"name": "surecart/product-media",
	"title": "Product Media",
	"description": "A container to display the product media",
	"category": "surecart",
	"keywords": ["image", "media"],
	"supports": {
		"reusable": false,
		"html": false,
		"multiple": false
	},
	"attributes": {
		"auto_height": {
			"type": "boolean",
			"default": true
		},
		"height": {
			"type": "string",
			"default": "310px"
		},
		"width": {
			"type": "number"
		},
		"thumbnails_per_page": {
			"type": "number",
			"default": 5
		}
	},
	"style": "file:./style.css"
}
